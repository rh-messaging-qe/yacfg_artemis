{% if user_jdbc_store is defined %}
  store:
    - type: jdbc
      properties:
        jdbc_driver_class_name: {{ user_jdbc_store.jdbc_driver_class_name }}
        jdbc_connection_url: {{ user_jdbc_store.jdbc_connection_url }}
        jdbc_connection_url_short: {{ user_jdbc_store.db_jdbc_url | default('None') }}
        jdbc_masked_username: {{ user_jdbc_store.db_username_masked | default('None') }}
        jdbc_masked_password: {{ user_jdbc_store.db_password_masked | default('None') }}
        {% if user_jdbc_store.data_source_class_name is defined %}
        data_source_class_name: {{ user_jdbc_store.data_source_class_name }}
        {% endif %}
        {% if user_jdbc_store.data_source_properties is defined %}
        data_source_properties: {{ user_jdbc_store.data_source_properties }}
        {% endif %}
        message_table_name: {{ user_jdbc_store.message_table_name | default('MESSAGE_TABLE') }}
        bindings_table_name: {{ user_jdbc_store.bindings_table_name | default('BINDINGS_TABLE') }}
        large_message_table_name: {{ user_jdbc_store.large_message_table_name | default('LARGE_MESSAGES_TABLE') }}
        page_store_table_name: {{ user_jdbc_store.page_store_table_name | default('PAGE_TABLE') }}
        node_manager_store_table_name: {{ user_jdbc_store.node_manager_store_table_name | default('NODE_MANAGER_TABLE')}}
        {% if user_jdbc_store.jdbc_network_timeout is defined %}
        jdbc_network_timeout: {{ user_jdbc_store.jdbc_network_timeout }}
        {% endif %}
        {% if user_jdbc_store.jdbc_lock_renew_period is defined %}
        jdbc_lock_renew_period: {{ user_jdbc_store.jdbc_lock_renew_period }}
        {% endif %}
        {% if user_jdbc_store.jdbc_lock_expiration is defined %}
        jdbc_lock_expiration: {{ user_jdbc_store.jdbc_lock_expiration }}
        {% endif %}
        {% if user_jdbc_store.jdbc_journal_sync_period is defined %}
        jdbc_journal_sync_period: {{ user_jdbc_store.jdbc_journal_sync_period }}
        {% endif %}
        {% if user_jdbc_store.jdbc_allowed_time_diff is defined %}
        jdbc_allowed_time_diff: {{ user_jdbc_store.jdbc_allowed_time_diff }}
        {% endif %}
{% endif %}

